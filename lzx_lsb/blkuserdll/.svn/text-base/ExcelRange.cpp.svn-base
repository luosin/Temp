CExcelRange::CExcelRange()
{
	
	iStartRow = 0 ;
	iEndRow = 0 ;
	iStartCol = 0 ;
	iEndCol = 0 ;
}
CExcelRange::CExcelRange(int iStartRow, int iEndRow, int iStartCol, int iEndCol) 
{
	this->iStartRow = iStartRow ;
	this->iEndRow = iEndRow ;
	this->iStartCol = iStartCol ;
	this->iEndCol = iEndCol ;
}

CExcelRange::~CExcelRange()
{

